/dts-v1/;
#include "rk3288-evb-android-rk808-edp.dtsi"

/delete-node/ &vcc_wl;
/delete-node/ &vcc_lcd;

/ {
	model = "ONYX MPAD-800";
	compatible = "rockchip,rk3288-evb-android-rk808-edp", "rockchip,rk3288";
	chosen: chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff690000 vmalloc=496M swiotlb=1 console=ttyFIQ0 androidboot.baseband=N/A androidboot.veritymode=enforcing androidboot.hardware=rk30board androidboot.console=ttyFIQ0 init=/init kpti=0";
	};

	panel {
		status = "disabled";
	};

	vcc_lcd: vcc-lcd {
		compatible = "regulator-fixed";
		regulator-name = "vcc_lcd";
		gpio = <&gpio2 RK_PA0 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <20000>;
		enable-active-high;
		regulator-boot-on;
	};

	vusb2_5v: vusb2-5v {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio0 RK_PC1 GPIO_ACTIVE_HIGH>; /* USBA_EN */
		pinctrl-names = "default";
		pinctrl-0 = <&usba_en>;
		regulator-name = "vusb2_5v";
		regulator-always-on;
	};

	wireless-wlan {
		wifi_chip_type = "ap6255";
	};
};

&i2c2 {
	/delete-node/ rt5640@1c;

	rt5670: rt5670@1c {
		#sound-dai-cells = <0>;
		compatible = "realtek,rt5670";
		reg = <0x1c>;
		clocks = <&cru SCLK_I2S0_OUT>;
		clock-names = "mclk";
		interrupt-parent = <&gpio6>;
		interrupts = <7 IRQ_TYPE_EDGE_FALLING>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2s0_mclk>;
	};
};

&i2c4 {
	/delete-node/ gsl3673@40;

	touch@38 {
		compatible = "focaltech,fts";
		status = "okay";
		reg = <0x38>;
		focaltech,reset-gpio = <&gpio2 RK_PA2 GPIO_ACTIVE_HIGH>;
		focaltech,irq-gpio = <&gpio2 RK_PA3 IRQ_TYPE_EDGE_FALLING>;
		focaltech,max-touch-number = <5>;
		focaltech,display-coords = <0 0 800 1280>;
		pinctrl-names = "default";
		pinctrl-0 = <&fts_irq_gpio>;
	};
};

&io_domains {
	status = "okay";

	audio-supply = <&vcc_18>;
	dvp-supply = <&vcc_18>;
	flash0-supply = <&vcc_io>;
	flash1-supply = <&vio_1v8>;
	lcdc-supply = <&vcc_io>;
	wifi-supply = <&vcc_18>;
};

&isp_mmu {
	status = "okay";
};

&backlight {
	/delete-property/ enable-gpios;
	pwms = <&pwm0 0 25000 0>;
};

&dsi0 {
	status = "okay";
	panel@0 {
		compatible = "simple-panel-dsi";
		reg = <0>;
		backlight = <&backlight>;
		power-supply = <&vcc_lcd>;
		reset-gpios = <&gpio2 RK_PA1 GPIO_ACTIVE_LOW>;
		pinctrl-0 = <&lcd_rst>;
		prepare-delay-ms = <100>;
		enable-delay-ms = <120>;
		reset-delay-ms = <20>;
		init-delay-ms = <20>;

		dsi,flags = <(MIPI_DSI_MODE_VIDEO | MIPI_DSI_MODE_VIDEO_BURST |
			     MIPI_DSI_MODE_LPM | MIPI_DSI_MODE_EOT_PACKET)>;
		dsi,format = <MIPI_DSI_FMT_RGB888>;
		dsi,lanes = <4>;

		panel-init-sequence = [
			39 00 04 b9 ff 83 94
			39 00 05 d9 00 8b 02 07
			39 00 03 ba 73 83
			39 00 10 b1 6c 15 15 24 e4 11 f1 80 e8 de 23 80 c0 d2 58
			39 00 0b b2 00 64 10 07 20 1c 08 08 1c 4d
			39 00 0d b4 00 ff 03 5a 03 5a 03 5a 01 6a 01 6a
			39 00 03 b6 40 40
			15 00 02 cc 01
			39 00 1f d3 00 06 00 40 1a 08 00 32 10 07 00 07 54 15 0f 05 04 02 12 10 05 07 33 33 0b 0b 37 10 07 07
			39 00 2d d5 19 19 18 18 1a 1a 1b 1b 04 05 06 07 00 01 02 03 20 21 18 18 18 18 18 18 18 18 18 18 18 18 22 23 18 18 18 18 18 18 18 18 18 18 18 18
			39 00 2d d6 18 18 19 19 1a 1a 1b 1b 03 02 01 00 07 06 05 04 23 22 18 18 18 18 18 18 18 18 18 18 18 18 21 20 18 18 18 18 18 18 18 18 18 18 18 18
			39 00 2b e0 00 06 0c 31 34 3f 1d 41 06 0a 0c 17 0f 12 15 13 14 07 12 15 16 00 06 0b 30 34 3f 1f 40 07 0a 0d 18 0e 12 14 12 14 08 13 14 19
			15 00 02 cc 09
			15 00 02 d2 55
			39 00 03 c0 30 14
			39 00 04 bf 41 0e 01
			39 00 05 c7 00 c0 40 c0
			15 00 02 df 8e
			05 00 01 11
			05 08 01 29
		];

		display-timings {
			native-mode = <&timing_dsi>;

			timing_dsi: timing_dsi {
				clock-frequency = <67000000>;
				hactive = <800>;
				vactive = <1280>;
				hback-porch = <18>;
				hfront-porch = <18>;
				vback-porch = <16>;
				vfront-porch = <6>;
				hsync-len = <18>;
				vsync-len = <4>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};
	};
};

&route_dsi0 {
	status = "okay";
};

&edp {
	status = "disabled";
};

&route_edp {
	status = "disabled";
};

&hdmi {
	status = "disabled";
};

&route_hdmi {
	status = "disabled";
};

&vopb {
	status = "disabled";
};

&vopl {
	assigned-clocks = <&cru DCLK_VOP1>;
	assigned-clock-parents = <&cru PLL_CPLL>;
};

&rk808 {
	vcc7-supply = <&vcc_sys>;
	vcc9-supply = <&vcc_sys>;
	vcc10-supply = <&vcc_sys>;
	vcc11-supply = <&vcc_sys>;

	regulators {
		DCDC_REG1 {
			regulator-min-microvolt = <700000>;
			regulator-max-microvolt = <1500000>;
			regulator-state-mem {
				regulator-suspend-microvolt = <900000>;
			};

		};

		DCDC_REG2 {
			regulator-min-microvolt = <700000>;
			regulator-max-microvolt = <1500000>;
			regulator-state-mem {
				regulator-suspend-microvolt = <900000>;
			};
		};

		LDO_REG1 {
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <2800000>;
			regulator-state-mem {
				regulator-suspend-microvolt = <2800000>;
			};
		};

		vio_1v8: LDO_REG4 {
			regulator-always-on;
			regulator-boot-on;
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-name = "vio_1v8";
			regulator-state-mem {
				regulator-off-in-suspend;
				regulator-suspend-microvolt = <1800000>;
			};
		};

		LDO_REG5 {
			regulator-min-microvolt = <3300000>;
		};

		LDO_REG6 {
			regulator-state-mem {
				regulator-suspend-microvolt = <1000000>;
			};
		};

		LDO_REG8 {
			/delete-node/ regulator-always-on;
			/delete-node/ regulator-boot-on;
			regulator-state-mem {
				regulator-suspend-microvolt = <1800000>;
			};
		};

		SWITCH_REG1 {
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-state-mem {
				regulator-suspend-microvolt = <3300000>;
			};
		};

		vcc_gps: SWITCH_REG2 {
			regulator-always-on;
			regulator-boot-on;
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-name = "vcc_gps";
			regulator-state-mem {
				regulator-off-in-suspend;
			};
		};
	};
};

&pinctrl {
	fts {
		fts_irq_gpio: fts-irq-gpio {
			rockchip,pins = <2 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	lcd {
		lcd_rst: lcd-rst {
			rockchip,pins = <2 RK_PA1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb {
		usba_en: usba-en {
			rockchip,pins = <0 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

&sound {
	simple-audio-card,name = "realtek,rt5670-codec";

	simple-audio-card,dai-link@0 {
		codec {
			sound-dai = <&rt5670>;
		};
	};

	/delete-node/ simple-audio-card,dai-link@1;
};
